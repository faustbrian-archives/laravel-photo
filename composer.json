{
    "name": "kodekeep/laravel-photo",
    "description": "Attach Photos to Laravel Eloquent Models.",
    "keywords": [
        "kodekeep",
        "laravel-photo"
    ],
    "homepage": "https://github.com/kodekeep/laravel-photo",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "brian@kodekeep.com",
            "homepage": "https://kodekeep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^6.0",
        "spatie/laravel-medialibrary": "^7.18"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "graham-campbell/analyzer": "^2.4",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "KodeKeep\\Photo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeKeep\\Photo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=coverage.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
